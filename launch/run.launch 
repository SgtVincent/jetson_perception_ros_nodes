<!-- This is the default launch file for the package -->

<launch>

    <!-- Load default parameters from config yaml file -->
    <rosparam command="load" file="$(find jetson_perception_ros_nodes)/config/default.yaml" />
    <!-- monocular depth estimation node -->
    <node name="mono_depth_node" pkg="jetson_perception_ros_nodes" type="mono_depth_node.py" output="screen"/>

</launch>